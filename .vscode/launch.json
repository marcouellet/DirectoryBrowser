            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/DirListTest/bin/Debug/net6.0/DirListTest.dll",
            "args": [],
